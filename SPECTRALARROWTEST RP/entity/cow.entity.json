{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:cow",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "entity_alphatest",

        "cow_glow": "entity_emissive",
        "cow_glow2": "entity_glow_status"
      },
      "textures": {
        "default": "textures/entity/cow/cow",

        "glowing_texture": "textures/entity/mob_glowing_effect/glow_highlight"
      },
      "geometry": {
        "default": "geometry.cow",

        "cow_glow": "geometry.cow_glow"
      },
      "animations": {
        "setup": "animation.cow.setup",
        "walk": "animation.quadruped.walk",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.cow.baby_transform"
      },
      "scripts": {
        "animate": [
          "setup",
          {
            "walk": "query.modified_move_speed"
          },
          "look_at_target",
          {
            "baby_transform": "query.is_baby"
          }
        ]
      },
      "render_controllers": [
        "controller.render.cow",

        "controller.render.cow_glow",
        "controller.render.cow_glow2"
      ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 1
      }
    }
  }
}